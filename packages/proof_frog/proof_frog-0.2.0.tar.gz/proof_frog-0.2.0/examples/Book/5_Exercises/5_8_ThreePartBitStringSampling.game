Game Concatenate(Int len) {
    BitString<3 * len> Query() {
        BitString<len> x <- BitString<len>;
        BitString<len> y <- BitString<len>;
        BitString<len> z <- BitString<len>;
        return x || y || z;
    }
}

Game SampleDirectly(Int len) {
    BitString<3 * len> Query() {
        BitString<3 * len> value <- BitString<3 * len>;
        return value;
    }
}

export as ThreePartBitStringSampling;
