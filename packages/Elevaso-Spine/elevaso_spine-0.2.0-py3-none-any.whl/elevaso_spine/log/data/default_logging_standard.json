{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "standard": {
      "format": "[%(levelname)s]%(asctime)s..%(module)s.%(funcName)s..%(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S",
      "()": "elevaso_spine.log.fmt_standard.StandardFormatter",
      "timestamp_key": "timestamp",
      "session_key": "exec_id",
      "timezone": "UTC"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "standard",
      "stream": "ext://sys.stdout"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "console"
    ],
    "propogate": "no"
  },
  "loggers": {
    "elevaso-spine": {
      "level": "ERROR",
      "handlers": [
        "console"
      ],
      "propogate": "no"
    }
  }
}
